{
  "name": "goblog-client-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "lint": "ng lint",
    "commit": "cz",
    "dev:ssr": "ng run goblog-client-web:serve-ssr",
    "serve:ssr": "node dist/goblog-client-web/server/main.js",
